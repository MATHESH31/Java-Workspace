package Practice;

import java.util.*;

public class SeceDining {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
      	String door = in.nextLine();
      	in.nextLine();
      	int rail = in.nextInt();
      
      	if(door == "front" && rail == 1 || door == "rear" && rail == 2){
        	System.out.println("Left Handed");
        }
      	else if(door == "front" && rail == 2 || door == "rear" && rail == 1){
        	System.out.println("Right Handed");
        }
      in.close();

	}

}
