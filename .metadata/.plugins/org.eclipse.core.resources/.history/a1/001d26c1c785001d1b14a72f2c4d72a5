package Practice;

import java.util.*;
import java.io.*;

public class End_Of_File {
	
	public static void main(String[] args) throws IOException {
		Scanner in = new Scanner(System.in);
        int b = 1;
        while(in.hasNextLine()){
            String a = in.nextLine();
            System.out.println(b+ " "+a);
            b++;
        }
	}

}
