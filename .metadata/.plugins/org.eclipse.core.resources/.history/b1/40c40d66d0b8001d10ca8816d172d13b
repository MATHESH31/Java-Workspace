package ThreadAndTransaction;

import java.util.Date;

public class Transactions {

	private String Transaction_id;
	private String From_account_id;
	private String To_account_id;
	private Date Date;
	private int Amount;
	private int Balance;
	
	public Transactions(String transaction_id, String from_account_id, String to_account_id, java.util.Date date,
			int amount, int balance) {
		this.Transaction_id = transaction_id;
		this.From_account_id = from_account_id;
		this.To_account_id = to_account_id;
		this.Date = date;
		this.Amount = amount;
		this.Balance = balance;
	}
	
	
}
