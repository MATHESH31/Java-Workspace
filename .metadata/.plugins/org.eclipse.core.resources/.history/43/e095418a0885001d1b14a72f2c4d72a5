package Practice;

import java.util.*;

public class game_2046 {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
        int a = in.nextInt();
        String[] b = in.nextLine().split(" ");
        String[] c = in.nextLine().split(" ");
        
        for(int i=1;i<=Integer.parseInt(b[2]);i++){
            int d = 1;
            int e = Integer.parseInt(b[0])+(d*Integer.parseInt(b[1]));
            System.out.println(e);
            e += (d*Integer.parseInt(b[1]));
            d *= 2;
        }
        
        for(int i=1;i<=Integer.parseInt(c[2]);i++){
            int d = 1;
            int e = Integer.parseInt(c[0])+(d*Integer.parseInt(c[1]));
            System.out.println(e);
            e += (d*Integer.parseInt(c[1]));
            d *= 2;
        }
        in.close();
        
	}

}
