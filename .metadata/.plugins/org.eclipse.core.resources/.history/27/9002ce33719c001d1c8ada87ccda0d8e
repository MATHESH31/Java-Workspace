package AutomatedTellerMachine;

public class Balance {

	private int id;
	private int amount;
	
	@Override
	public String toString() {
		return "Balance [id = " + id + ", amount = " + amount + "]";
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getAmount() {
		return amount;
	}

	public void setAmount(int amount) {
		this.amount = amount;
	}
	
	
	
}
